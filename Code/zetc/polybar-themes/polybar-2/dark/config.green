;==========================================================

[bar/main]
width = 100%
height = 42
offset-x = 2%
offset-y = 2%
bottom = false
fixed-center = true
line-size = 3
radius = 0

background = ${color.bg}
foreground = ${color.fg}

border-size = 10
border-color = #00000000

tray-position = center

;This DPI looks good on the laptop
dpi = 192

locale = es_AR.UTF-8
;en_US.UTF-8

;==========================================================

font-0 = "Fantasque Sans Mono:pixelsize=10;3"
font-1 = "Iosevka Nerd Font:pixelsize=13;3"

;==========================================================

cursor-click = pointer
cursor-scroll = ns-resize

scroll-up = i3wm-wsnext
scroll-down = i3wm-wsprev

;== Module Location ========================================================

modules-left = workspaces
modules-center = date
modules-right = mpd pkg volume xbacklight battery network powermenu

;override-redirect = true
;wm-restack = i3

